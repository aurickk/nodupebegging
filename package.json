{
  "name": "nodupebegging.com",
  "description": "please don't beg for dupes in discord",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/aurickk/nodupebegging"
  },
  "type": "commonjs",
  "scripts": {
    "11ty": "node node_modules/@11ty/eleventy/cmd.js --config .eleventy.ts",
    "build": "npm run 11ty",
    "serve": "npm run 11ty -- --serve --port=8123",
    "format": "prettier --write .",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.23",
    "esbuild": "^0.14.28",
    "esbuild-runner": "^2.2.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^2.0.0",
    "glob": "^7.2.0",
    "marked": "^4.0.12",
    "nunjucks": "^3.2.3",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0"
  },
  "resolutions": {
    "@11ty/eleventy": "^1.0.0",
    "printf": "^0.6.1"
  }
